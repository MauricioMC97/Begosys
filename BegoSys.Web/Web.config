<configuration>
  <configSections>
    <sectionGroup name="spring">
      <section name="context" type="Spring.Context.Support.ContextHandler, Spring.Core" />
      <section name="objects" type="Spring.Context.Support.DefaultSectionHandler, Spring.Core" />
    </sectionGroup>
    <sectionGroup name="common">
      <section name="logging" type="Common.Logging.ConfigurationSectionHandler, Common.Logging" />
    </sectionGroup>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,log4net" />
  </configSections>
  <appSettings>
    <add key="log4net.Internal.Debug" value="true" />
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <!-- Custom settings -->
    <add key="TimeoutSecondsSettingsKey" value="600" />
    <add key="MinutesExpirationCache" value="30" />
    <add key="DefaultDomainAPI" value="" />
    <add key="Cultura" value="en-US" />
    <add key="FormatoFechaSistema" value="dd/MM/yyyy" />
    <add key="FormatoFechaMes" value="MM/yyyy" />
    <add key="FormatoHoraSistema" value="HH:mm" />
    <add key="SeparadorDecimalSistema" value="." />
    <add key="SeparadorMilesSistema" value="ç" />
    <add key="FormatoMoneda" value="$" />
    <add key="APIRoot" value="http://localhost:3445/JBCService" />
    <add key="WebRoot" value="http://localhost/JBCWeb" />
    <add key="DefaultDomain" value="" />
    <!--<add key="UserNameAPI" value="apiuser_sgdhm" />-->
    <!--<add key="PasswordAPI" value="mvm2017*" />-->
    <add key="NombreCadenaConexion" value="OracleConnectionString" />
    
    <!--Desarrollo-->
    <!--<add key="EsquemaBaseDatos" value="JBDLLO" />-->
    
    <!--Producción-->
    <add key="EsquemaBaseDatos" value="JUICEBAR" />
    
    <add key="RolesSistema" value="R_DHIMEMP_CAPTURA,R_DHIMEMP_CONSULTA,R_DHIMEMP_ADMIN,R_DHIMEMP_ADMINCNE" />
    <add key="aspnet:MaxJsonDeserializerMembers" value="150000" />
  </appSettings>
  <common>
 <logging>
      <factoryAdapter type="Common.Logging.Log4Net.Log4NetLoggerFactoryAdapter, Common.Logging.Log4Net1213">
        <arg key="configType" value="FILE-WATCH" />
        <arg key="configFile" value="~/Configuration/log4net.xml" />
      </factoryAdapter>
    </logging>
  </common>
  <spring>
    <context>
      <resource uri="~/Configuration/Infrastructure.xml" />
      <resource uri="~/Configuration/Business.xml" />
    </context>
  </spring>
  <system.web>
    <securityPolicy>
      <trustLevel name="Full" policyFile="internal" />
    </securityPolicy>
    <compilation debug="true" targetFramework="4.6.2" />
    <httpRuntime targetFramework="4.6.2" maxUrlLength="10999" maxQueryStringLength="2097151" />
    <authentication mode="Windows" />
    <authorization>
      <allow users="*" />
      <deny users="?" />
    </authorization>
    <customErrors mode="Off" />
    <httpModules>
      <!--<add name="ApplicationInsightsWebTracking" type="Microsoft.ApplicationInsights.Web.ApplicationInsightsHttpModule, Microsoft.AI.Web" />-->
    </httpModules>
    <pages>
      <namespaces>
        <add namespace="System.Web.Mvc" />
        <add namespace="System.Web.Mvc.Ajax" />
        <add namespace="System.Web.Mvc.Html" />
        <add namespace="System.Web.Routing" />
      </namespaces>
    </pages>
    <httpHandlers>
      <add path="Reserved.ReportViewerWebControl.axd" verb="*" type="Microsoft.Reporting.WebForms.HttpHandler, Microsoft.ReportViewer.WebForms, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91" validate="false" />
    </httpHandlers>
    <sessionState timeout="180" cookieless="false" mode="InProc"></sessionState>
  </system.web>
