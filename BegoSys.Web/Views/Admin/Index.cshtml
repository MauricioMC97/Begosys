    <script type="text/javascript">

        function calculateLineTotal(numunit, rate, total) {
            var rate = document.getElementById(rate).value;
            var numunit = document.getElementById(numunit).value;


            var sum = numunit * rate;


            document.getElementById(total).value = sum;
            findTotal();
        }
    </script>

    <script type="text/javascript">
        function findTotal() {
            var arr = document.getElementsByName('Subtotal');
            var tot = 0;
            for (var i = 0; i < arr.length; i++) {
                if (parseInt(arr[i].value))
                    tot += parseInt(arr[i].value);
            }
            document.getElementById('ValorTotal').value = tot;
            document.getElementById('Impoconsumo').value = tot * 0.08;
        }

